{
  "name": "atenmed-backend",
  "version": "1.0.0",
  "description": "Backend API para AtenMed - Organização Inteligente para Consultórios",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "pm2": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop atenmed",
    "pm2:restart": "pm2 restart atenmed",
    "pm2:delete": "pm2 delete atenmed",
    "pm2:logs": "pm2 logs atenmed",
    "pm2:monit": "pm2 monit",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:sentry": "node scripts/test-sentry.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "node -e \"try { require('husky').install() } catch(e) {}\" || true",
    "test-webhook": "node test-webhook.js",
    "test-webhook-local": "node test-webhook-local.js",
    "test-whatsapp": "node test-whatsapp-completo.js",
    "test-webhook-msg": "node test-webhook-mensagem.js",
    "build": "npm run build:css",
    "build:css": "npx tailwindcss -i ./site/assets/css/tailwind-input.css -o ./site/assets/css/tailwind.css --minify",
    "watch:css": "npx tailwindcss -i ./site/assets/css/tailwind-input.css -o ./site/assets/css/tailwind.css --watch",
    "init-db": "node scripts/init-db.js",
    "logs": "tail -f logs/combined.log",
    "deploy": "chmod +x deploy.sh && ./deploy.sh"
  },
  "keywords": [
    "atenmed",
    "medical",
    "api",
    "consultorios",
    "whatsapp",
    "automation"
  ],
  "author": "AtenMed Team",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@bull-board/ui": "^6.14.0",
    "@sentry/node": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bottleneck": "^2.19.5",
    "bull": "^4.16.5",
    "chalk": "^4.1.2",
    "chart.js": "^4.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.0",
    "googleapis": "^126.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.9",
    "pm2": "^6.0.13",
    "postcss": "^8.5.6",
    "redis": "^5.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwindcss": "^4.1.16",
    "uuid": "^9.0.1",
    "whatsapp-cloud-api": "^0.3.1",
    "winston": "^3.11.0",
    "xss": "^1.0.14",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "tailwindcss-cli": "^0.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
