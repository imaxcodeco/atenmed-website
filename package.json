{
  "name": "atenmed-backend",
  "version": "1.0.0",
  "description": "Backend API para AtenMed - Organização Inteligente para Consultórios",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "pm2": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop atenmed",
    "pm2:restart": "pm2 restart atenmed",
    "pm2:delete": "pm2 delete atenmed",
    "pm2:logs": "pm2 logs atenmed",
    "pm2:monit": "pm2 monit",
    "test": "jest",
    "build": "echo 'Build completed'",
    "init-db": "node scripts/init-db.js",
    "logs": "tail -f logs/combined.log",
    "deploy": "chmod +x deploy.sh && ./deploy.sh"
  },
  "keywords": [
    "atenmed",
    "medical",
    "api",
    "consultorios",
    "whatsapp",
    "automation"
  ],
  "author": "AtenMed Team",
  "license": "MIT",
